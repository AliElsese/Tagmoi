                                                    <div class="col-6">
                                                        <button type="reset">cancel</button>
                                                    </div>
                                                    <div class="col-6 d-flex justify-content-between">
                                                        <button type="submit">save as draft</button>
                                                        <button type="submit">publish</button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
