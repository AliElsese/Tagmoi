                                        <div class="col-12">
                                            <form action="" method="post">
                                                <div class="col-12">
                                                    <p class="head-para">section 1</p>
                                                </div>
                                                <div class="col-12 d-flex align-items-center">
                                                    <label for="header 1">header 1</label>
                                                    <input type="text" name="header-1" placeholder="add your text" style="margin-left: 9.3vw;">
                                                    <input type="color" name="font_color" id="colorStyle" value="#4ECB71">
                                                </div>
                                                <div class="col-12 d-flex align-items-center">
                                                    <label for="header 2">header 2</label>
                                                    <input type="text" name="header-2" placeholder="add your text" style="margin-left: 9.3vw;">
                                                    <input type="color" name="font_color" id="colorStyle" value="#00A2F4">
                                                </div>
                                                <div class="col-12 d-flex align-items-center">
                                                    <label for="paragraph">paragraph</label>
                                                    <input type="text" name="paragraph" placeholder="add your text" style="margin-left: 8.6vw;">
                                                    <input type="color" name="font_color" id="colorStyle" value="#EBF8FF">
                                                </div>
                                                <div class="col-12 d-flex align-items-center media">
                                                    <label for="choose media">choose media</label>
                                                    <label for="media">
                                                        <a for="media" style="cursor: pointer;">upload file</a>
                                                    </label>
                                                    <input type="file" name="media" id="media" style="display: none;visibility: none;">
                                                </div>
                                                <div class="col-12 d-flex align-items-center" style="margin-top: 1.9vw;">
                                                    <label for="button 1">button1</label>
                                                    <img src="/assets/img/google play.png" alt="" style="width: 9.6vw;margin-left: 9.7vw;">
